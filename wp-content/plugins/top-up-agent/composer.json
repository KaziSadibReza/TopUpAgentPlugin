{
  "require": {},
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "TopUpAgent\\": "includes/"
    },
    "classmap": [
      "includes/"
    ]
  },
  "scripts": {
